{
  "include": [
    "node_modules/@roboportal/**/*",
    "."
  ],
  "compilerOptions": { 
    "target": "es2021",
    "outDir": "dist",
    // "types": ["node"],
    // "lib": ["es2021"],
    "moduleResolution": "node",
    "module": "commonjs",
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}